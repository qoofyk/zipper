{
    "cmake.sourceDirectory": "${workspaceFolder}/lbm_adios",
    "cmake.buildDirectory": "${workspaceFolder}/lbm_adios/build",
    "C_Cpp.default.configurationProvider": "vector-of-bool.cmake-tools",
    "cmake.configureOnOpen": true,
    "files.associations": {
        "*.tcc": "c"
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/extern/**": true,
        ".venv/**": true
    },
    "python.pythonPath": ".venv/bin/python3"
}