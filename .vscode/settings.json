{
    "cmake.sourceDirectory": "${workspaceFolder}/lbm_adios",
    "cmake.buildDirectory": "${workspaceFolder}/lbm_adios/build",
    "C_Cpp.default.configurationProvider": "vector-of-bool.cmake-tools",
    "cmake.configureOnOpen": true,
    "files.associations": {
        "*.tcc": "c",
        "mySimpleFoam.C": "cpp",
        "Field.C": "cpp"
    },
    "files.watcherExclude": {
        ".venv/**": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/data/**": true,
        "**/metastore_db/**": true,
        "**/node_modules/*/**": true,
        "build/**": true,
        "extern/**": true
    },
    "python.pythonPath": ".venv/bin/python3"
}